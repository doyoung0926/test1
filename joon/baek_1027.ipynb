    "# https://www.acmicpc.net/problem/1977\n",
    "\n",
    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "M = int(input())\n",
    "N = int(input())\n",
    "\n",
    "value = [i for i in range(M, N+1) if i**(1/2) == int(i**(1/2))]\n",
    "if len(value) >= 1:\n",
    "    print(sum(value))\n",
    "    print(min(value))\n",
    "else:\n",
    "    print(-1)"
